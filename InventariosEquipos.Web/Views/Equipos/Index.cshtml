@model IEnumerable<InventariosEquipos.Web.Data.Entities.Equipo>

@{
    ViewData["Title"] = "Index";
}

<h2>Equipos IE Grupo</h2>

<p>
    <a asp-action="Create">Crear un equipo nuevo</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sucursal)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NombreEmpleado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoEquipo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MarcaEquipo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DireccionIP)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EstadoEquipo)
            </th>

            
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sucursal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NombreEmpleado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipoEquipo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MarcaEquipo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DireccionIP)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.EstadoEquipo)
            </td>

            
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
